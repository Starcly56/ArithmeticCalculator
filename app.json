{
  "expo": {
    "name": "My-Calculator",
    "slug": "My-Calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This is a simple arithmetic calculator developed in react native where operations like addition, subtraction, division, multiplication, percentage calculation and binary operations can be performed.",
    "githubUrl": "https://github.com/Starcly56/ArithmeticCalculator"
  }
}
